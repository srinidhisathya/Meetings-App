{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "import/prefer-default-export": 1,

        "no-console": 0,

        "class-methods-use-this": 0,

        "import/extensions": 0,

        "import/no-cycle": 0,

        "no-useless-escape": 1,

        "no-sequences": 0,

        "no-param-reassign": 0,

        "import/named": 1,

        "no-underscore-dangle": 0,

        "no-plusplus":0


    }
}
